{
  "name": "timer",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production && node ./scripts/gh-pages-before-add.js",
    "serve:ci": "npx local-web-server --port 4200 --directory ./dist --spa timer-5/404.html",
    "e2e": "testcafe chrome:headless -c 4 -q",
    "e2e:ci": "testcafe chrome:headless -c 4 -q --app=\"npm run serve:ci\" -r spec,xunit:e2e/report.xml --base-url=\"http://localhost:4200/timer-5/\"",
    "deploy": "gh-pages --nojekyll --dotfiles --dist dist/timer-5"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.5",
    "@angular/cdk": "^18.0.5",
    "@angular/common": "^18.0.5",
    "@angular/compiler": "^18.0.5",
    "@angular/core": "^18.0.5",
    "@angular/forms": "^18.0.5",
    "@angular/material": "^18.0.5",
    "@angular/platform-browser": "^18.0.5",
    "@angular/platform-browser-dynamic": "^18.0.5",
    "@angular/router": "^18.0.5",
    "@angular/service-worker": "^18.0.5",
    "@ngrx/signals": "18.0.0-rc.1",
    "angular2-hotkeys": "^16.0.1",
    "date-fns": "^3.6.0",
    "immer": "^10.1.1",
    "nanoid": "^5.0.7",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.3",
    "uplot": "^1.6.30"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular/cli": "^18.0.6",
    "@angular/compiler-cli": "^18.0.5",
    "@angular/language-service": "^18.0.5",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/node": "^20.14.9",
    "angular-testcafe": "^4.0.0",
    "chrome-remote-interface": "^0.33.0",
    "gh-pages": "^6.1.1",
    "local-web-server": "^5.3.3",
    "looks-same": "^9.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "testcafe": "^3.6.1",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=20.14.0"
  }
}
